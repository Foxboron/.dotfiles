"amixer -D pulse sset Master 5%- && pkill -RTMIN+10 i3blocks"
    XF86AudioLowerVolume

"amixer -D pulse sset Master 5%+ && pkill -RTMIN+10 i3blocks"
    XF86AudioRaiseVolume

"amixer -D pulse sset Master toggle && pkill -RTMIN+10 i3blocks"
    XF86AudioMute

"asus-kbd-backlight down"
    XF86KbdBrightnessDown

"asus-kbd-backlight up"
        XF86KbdBrightnessUp

"synclient TouchpadOff=$(synclient -l | grep -c 'TouchpadOff.*=.*0')"
    XF86TouchpadToggle

"xrandr -display :0 --output HDMI1 --mode 1920x1080 --above eDP1 --output eDP1 --primary"
    XF86Display

