#!/bin/bash
ROOT=`pwd`
readonly DIR=`echo $ROOT | awk -F"/" '{print $NF}'`
readonly YEAR=`echo $ROOT | awk -F"/" '{print $--NF}'`
if [ $# -eq 0 ]
  then
      NN=usortert
  else
      NN=$1
fi
echo $NN
# rsync -hpavzs *.jpg "pics:./$nn/albums/$dir $year"
rsync -hpavzs *.JPG "pics:./albums/$DIR $YEAR"
